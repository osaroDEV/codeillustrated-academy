{
  "name": "codeillustrated-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/root",
    "dev:academy": "npm run dev --workspace=apps/academy",
    "dev:labs": "npm run dev --workspace=apps/labs",
    "dev:all": "npm run dev --workspaces",
    "build": "npm run build --workspaces",
    "build:root": "npm run build --workspace=apps/root",
    "build:academy": "npm run build --workspace=apps/academy",
    "build:labs": "npm run build --workspace=apps/labs",
    "start": "npm run start --workspace=apps/root",
    "start:academy": "npm run start --workspace=apps/academy",
    "start:labs": "npm run start --workspace=apps/labs",
    "lint": "npm run lint --workspaces"
  }
}
